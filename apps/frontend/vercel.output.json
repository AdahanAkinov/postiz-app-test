{
  "version": 3,
  "routes": [
    {
      "src": "^/api/(.*)",
      "dest": "https://postiz-app-test-production.up.railway.app/api/$1"
    },
    {
      "src": "^/uploads/(.*)",
      "dest": "https://postiz-app-test-production.up.railway.app/api/uploads/$1"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
} 